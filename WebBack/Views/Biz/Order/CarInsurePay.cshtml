@model WebBack.Models.Biz.Order.CarInsurePayViewModel
@{
    ViewBag.Title = "CarInsureFollow";
    Layout = "~/Views/Shared/_LayoutHomeDialog.cshtml";
}

@section styles{
    <style type="text/css">
        .f-tb1-detail .t {
            width: 120px;
        }

        .f-tb1-detail .c {
            width: 280px;
        }
    </style>

}

@section scripts{
    <script type="text/javascript">

        $(document).ready(function () {
            $.formValidator.initConfig({ formID: "form1", onError: function (msg) { art.dialog.tips(msg); } });
            $(":radio[name='Model.Params.OfferId']").formValidator({ onShow: "选择一家报价公司", onFocus: "选择一家报价公司", onCorrect: "&nbsp;" }).inputValidator({ min: 1, onError: "选择一家报价公司" });
        });

        function submitCarInsurePay(fun) {

            //art.dialog.confirm('确定支付？', function () {
            //    var l_Data = $("#form1").serializeArray();
            //    l_Data.push({ name: "Model.Sn", value: obj.Sn });
            //    l_Data.push({ name: "Model.Price", value: 10000 });
            //    l_Data.push({ name: "Model.ProductType", value: obj.ProductType });
            //    $.lumos.postJson({
            //        isUseHandling: true,
            //        url: "../Order/PayTest",
            //        data: l_Data,
            //        success: function (data) {


            //        }
            //    });
            //});


            var isValid = $.formValidator.pageIsValid();
            if (isValid) {
                art.dialog.confirm('确定支付订单？', function () {
                    var l_Data = $("#form1").serializeArray();
                    var offerId = $(":radio[name='Model.Params.OfferId']:checked").val();

                    l_Data.push({ name: "Model.Params", value: "{\"offerId\":\"" + offerId + "\"}" });
                    $.lumos.postJson({
                        isUseHandling: true,
                        url: "../Order/PayConfirmTest",
                        data: l_Data,
                        success: function (d) {
                            fun(d);
                        }
                    });
                });
            }
        }


    </script>
}

<form id="form1" name="form1">
    @AntiForgery.GetHtml()
    <input type="hidden" name="Model.OrderSn" value="@Model.OrderToCarInsure.Sn" />
    <input type="hidden" name="Model.ProductType" value="@Model.OrderToCarInsure.ProductType" />
    <div class="gbr-row-title clearfix">
        <div class="pull-left">
            <h5>商户信息</h5>
        </div>
        <div class="pull-right f-tb1-item-pull-right">
            <h6></h6>
        </div>
    </div>

    <table class="f-tb1-detail">
        <tr>
            <td class="t">商户代码：</td>
            <td class="c">
                <span>@Model.Merchant.ClientCode</span>
            </td>
            <td class="t">商户名：</td>
            <td class="c">
                <span>@Model.Merchant.YYZZ_Name</span>
            </td>
        </tr>
        <tr>
            <td class="t">联系人：</td>
            <td class="c">
                <span>@Model.Merchant.ContactName</span>
            </td>
            <td class="t">联系电话：</td>
            <td class="c">
                <span>@Model.Merchant.ContactPhoneNumber</span>
            </td>
        </tr>
    </table>

    <div class="gbr-row-title clearfix">
        <div class="pull-left">
            <h5>险种信息</h5>
        </div>
        <div class="pull-right f-tb1-item-pull-right">
            <h6></h6>
        </div>
    </div>
    <table id="list_table" class="list-tb" cellpadding="0" cellspacing="0">
        <thead>
            <tr>
                <th style="width:5%" class="no">
                    序号
                </th>
                <th style="width:20%">
                    名称
                </th>
                <th style="width:20%">

                </th>
                <th style="width:20%">

                </th>
                <th style="width:20%">
                    投保明细
                </th>
            </tr>
        </thead>
        <tbody>

            @for (var i = 0; i < Model.OrderToCarInsureOfferKind.Count; i++)
