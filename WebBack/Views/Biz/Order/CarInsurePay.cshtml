                <tr>
                    <td class="no">@(i + 1)</td>
                    <td class="overflow tipitem">@Model.OrderToCarInsureOfferKind[i].KindName</td>
                    <td class="overflow tipitem">@(Model.OrderToCarInsureOfferKind[i].KindValue + Model.OrderToCarInsureOfferKind[i].KindUnit)</td>
